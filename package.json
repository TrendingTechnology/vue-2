    "deploy": "chmod 777 deploy.sh & ./deploy.sh",
    "build:app": "vue-cli-service build",
    "build:lib": "vue-cli-service build --target lib --name lbzui --dest lib --inline-vue src/components/index.ts"
  },
  "main": "lib/lbzui.common.js",
  "unpkg": "lib/lbzui.umd.min.js",
  "files": [
    "lib",
    "src"
  ],
  "dependencies": {
